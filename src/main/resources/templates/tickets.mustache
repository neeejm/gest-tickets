{{>layout/header}}
<!-- Page Heading -->
<h1 class="h3 mb-2 text-gray-800">Tickets List</h1>

<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Description</th>
                        <th>urgence</th>
                        <th>status</th>
                        <th>os</th>
                        <th>issue par</th>
                        <th>dev</th>
                    </tr>
                </thead>
                <tbody>
                    {{#tickets}}
                    <tr>
                        <td>{{description}}</td>
                        <td>{{urgence}}</td>
                        <td>{{status}}</td>
                        <td>{{os}}</td>
                        <td>{{client}}</td>
                        <td>{{dev}}</td>
                    </tr>
                    {{/tickets}}
                </tbody>
            </table>
        </div>
    </div>
</div>
{{>layout/footer}}